{
  "name": "req-data-management",
  "version": "0.0.1",
  "license": "UNLICENSED",
  "scripts": {
    "dev": "yarn stop && concurrently \"yarn dev-server\" \"yarn expose\"",
    "dev-server": "ts-node-dev --watch ./.env ./src/app.ts",
    "start": "yarn server",
    "server": "nodemon ./src/app.ts",
    "expose": "mapps tunnel:create -p 8080",
    "stop": "kill-port 8080 && kill-port 4049 && kill-port 4040"
  },
  "dependencies": {
    "axios": "^1.7.2",
    "body-parser": "^1.19.0",
    "dotenv": "^5.0.1",
    "express": "^4.19.2",
    "jsonwebtoken": "^9.0.2",
    "monday-sdk-js": "^0.5.5",
    "mysql": "^2.18.1",
    "mysql2": "^3.10.0",
    "querystring": "^0.2.1",
    "ts-node": "^10.9.2"
  },
  "devDependencies": {
    "@mondaycom/apps-cli": "^2.6.1",
    "@types/express": "^4.17.21",
    "@types/jsonwebtoken": "^8.5.9",
    "@types/node": "^20.14.2",
    "concurrently": "^8.2.2",
    "cross-port-killer": "^1.4.0",
    "debug": "~2.6.9",
    "eslint": "^6.8.0",
    "nodemon": "^3.1.3",
    "ts-node-dev": "^1.1.8",
    "typescript": "^4.9.5"
  }
}